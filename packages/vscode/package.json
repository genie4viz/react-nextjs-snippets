{
  "name": "react-nextjs-snippets",
  "displayName": "React and Next.js Snippets",
  "description": "This is an extension for React and Next.js Snippets with Typescript support as well!",
  "version": "1.2.4",
  "publisher": "AvneeshAgarwal",
  "icon": "logo.png",
  "repository": {
    "url": "https://github.com/avneesh0612/react-nextjs-snippets"
  },
  "engines": {
    "vscode": "^1.64.0"
  },
  "scripts": {
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/react-javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react-javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/react-typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react-typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/next-javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/next-typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/next-typescript.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/next-javascript.json"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {}
}
